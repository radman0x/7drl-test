{
  "version": 1,
  "projects": {
    "rl-ui": {
      "projectType": "library",
      "root": "libs/rl-ui",
      "sourceRoot": "libs/rl-ui/src",
      "prefix": "scratch2",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/rl-ui/tsconfig.lib.json",
              "libs/rl-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/rl-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/rl-ui/jest.config.js",
            "tsConfig": "libs/rl-ui/tsconfig.spec.json",
            "setupFile": "libs/rl-ui/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "rad-pixi": {
      "root": "libs/rad-pixi",
      "sourceRoot": "libs/rad-pixi/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/rad-pixi/tsconfig.lib.json",
              "libs/rad-pixi/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/rad-pixi/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/rad-pixi/jest.config.js",
            "tsConfig": "libs/rad-pixi/tsconfig.spec.json"
          }
        }
      }
    },
    "rl-ecs": {
      "root": "libs/rl-ecs",
      "sourceRoot": "libs/rl-ecs/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/rl-ecs/tsconfig.lib.json",
              "libs/rl-ecs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/rl-ecs/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/rl-ecs/jest.config.js",
            "tsConfig": "libs/rl-ecs/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "rl-ui"
}
